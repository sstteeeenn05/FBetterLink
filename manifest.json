{
  "manifest_version": 3,
  "name": "FBetterLink",
  "description": "Say goodbye to FaceBook link tracker",
  "version": "1.0",
  "permissions":["storage","activeTab","scripting","tabs"],
  "host_permissions":["https://*.facebook.com/*"],
  "background":{
    "service_worker":"background.js"
  },
  "action": {
    "default_popup": "FBetterLink.html",
    "default_icon": "FBetterLink.png"
  }
}